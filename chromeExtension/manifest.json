{
  "manifest_version": 2,
  "name": "CheaterGPT",
  "version": "1.0",
  "description": "This is my extension.",
  "icons": {
    "16": "/images/logo_16x16.png",
    "48": "/images/logo_48x48.png",
    "128": "/images/logo_128x128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
      "default_title": "My Browser Action"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
